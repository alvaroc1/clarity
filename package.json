{
  "name": "clarity",
  "author": "Alvaro Carrasco",
  "description": "A remote canvas",
  "version": "0.0.1",
  "main": "build/main.js",
  "license": "MIT",
  "scripts": {
    "main2": "ELECTRON_ENABLE_LOGGING=1 rollup -c && cp node_modules/electron/path.txt build/path.txt && electron --trace-warnings .",
    "main": "NODE_OPTIONS=\"--unhandled-rejections=strict\" ELECTRON_ENABLE_LOGGING=1 rollup -c && electron --trace-warnings .",
    "build": "ELECTRON_ENABLE_LOGGING=0 rollup -c && cp node_modules/electron/path.txt build/path.txt",
    "make-osx": "electron-forge make --targets=dmg --arch=arm64,x64",
    "make-win": "electron-forge make --targets=squirrel --arch=x64",
    "make-deb": "electron-forge make --targets=deb --arch=x64",
    "-deleted-sign-arm64": "electron-osx-sign out/Clarity-darwin-arm64/Clarity.app --identity='Apple Development' --provisioning-profile=./Clarity_Development.profile",
    "test": "jest"
  },
  "config": {
    "forge": "forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.4",
    "@electron-forge/maker-deb": "6.0.4",
    "@electron-forge/maker-dmg": "6.0.4",
    "@electron-forge/maker-rpm": "6.0.4",
    "@electron-forge/maker-squirrel": "6.0.4",
    "@electron-forge/maker-zip": "6.0.4",
    "@electron/osx-sign": "1.0.4",
    "@rollup/plugin-commonjs": "24.0.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-replace": "4.0.0",
    "@rollup/plugin-typescript": "10.0.1",
    "@types/jest": "^28.1.6",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "electron": "22.0.0",
    "electron-squirrel-startup": "1.0.0",
    "jest": "^28.1.3",
    "react-devtools": "^4.10.1",
    "rollup": "2.77.4-1",
    "rollup-plugin-copy": "3.4.0",
    "ts-jest": "^28.0.7",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/joy": "^5.0.0-alpha.39",
    "@mui/material": "5.9.3",
    "electron-store": "^8.1.0",
    "jss": "10.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.4.1"
  },
  "packageManager": "yarn@3.2.1"
}
